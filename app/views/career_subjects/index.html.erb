<p id="notice"><%= notice %></p>

<h1>Listing Career Subjects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @career_subjects.each do |career_subject| %>
      <tr>
        <td><%= link_to 'Show', career_subject %></td>
        <td><%= link_to 'Edit', edit_career_subject_path(career_subject) %></td>
        <td><%= link_to 'Destroy', career_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Career subject', new_career_subject_path %>
