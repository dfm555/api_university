<p id="notice"><%= notice %></p>

<h1>Listing Teacher Careers</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Career</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_careers.each do |teacher_career| %>
      <tr>
        <td><%= teacher_career.teacher %></td>
        <td><%= teacher_career.career %></td>
        <td><%= link_to 'Show', teacher_career %></td>
        <td><%= link_to 'Edit', edit_teacher_career_path(teacher_career) %></td>
        <td><%= link_to 'Destroy', teacher_career, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher career', new_teacher_career_path %>
